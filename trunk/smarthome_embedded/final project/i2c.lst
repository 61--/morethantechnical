 491:../i2c.c      **** tS8 
 492:../i2c.c      **** i2cPoll(tU8 addr)
 493:../i2c.c      **** {
 494:../i2c.c      ****   tS8 retCode  = I2C_CODE_OK;
 495:../i2c.c      ****   tU8 status   = 0;
 496:../i2c.c      ****   volatile tU8 deviceReady = FALSE;
 497:../i2c.c      **** 
 498:../i2c.c      ****   while(deviceReady == FALSE)
 499:../i2c.c      ****   {
 500:../i2c.c      ****     //Generate Start condition
 501:../i2c.c      ****     retCode = i2cStart();
 502:../i2c.c      **** 
 503:../i2c.c      ****     //Transmit SLA+W
 504:../i2c.c      ****     if(retCode == I2C_CODE_OK)
 505:../i2c.c      ****     {
 506:../i2c.c      ****       //write SLA+W
 507:../i2c.c      ****       retCode = i2cPutChar(addr);
 508:../i2c.c      ****       while(retCode == (tS8)I2C_CODE_BUSY)
 509:../i2c.c      ****         retCode = i2cPutChar(addr);
 510:../i2c.c      ****     }
 511:../i2c.c      **** 
 512:../i2c.c      ****     if(retCode == I2C_CODE_OK)
 513:../i2c.c      ****     {
 514:../i2c.c      ****       //Wait until SLA+W transmitted
 515:../i2c.c      ****       //Get new status
 516:../i2c.c      ****       status = i2cCheckStatus();
 517:../i2c.c      **** 
 518:../i2c.c      ****       if(status == 0x18)
 519:../i2c.c      ****       {
 520:../i2c.c      ****         //data transmitted and ACK received
 521:../i2c.c      ****         deviceReady = TRUE;
 522:../i2c.c      ****       }
 523:../i2c.c      ****       else if(status == 0x20)
 524:../i2c.c      ****       {
 525:../i2c.c      ****         //data transmitted and ACK not received
 526:../i2c.c      ****         //send start bit, start again
 527:../i2c.c      ****         deviceReady = FALSE;
 528:../i2c.c      ****       }
 529:../i2c.c      ****       else if( status != 0xf8 )
 530:../i2c.c      ****       {
 531:../i2c.c      ****         //error
 532:../i2c.c      ****         retCode = I2C_CODE_ERROR;
 533:../i2c.c      ****         deviceReady = TRUE;
 534:../i2c.c      ****       }
 535:../i2c.c      ****     }
 536:../i2c.c      **** 
 537:../i2c.c      ****     //Generate Stop condition
 538:../i2c.c      ****     i2cStop();
 539:../i2c.c      ****   }
 540:../i2c.c      **** 
 541:../i2c.c      ****   return retCode;
 542:../i2c.c      **** }
